  // NOTE: 選択している範囲を少しずつ広げてくれるexpand_regionの設定
  {
    "command": "expand_region",
    "key": "ctrl+=",
    "when": "editorTextFocus"
  },

  // NOTE: 選択している範囲を少しずつ狭めてくれるundo_expand_regionの設定
  {
    "command": "undo_expand_region",
    "key": "ctrl+-",
    "when": "editorTextFocus && editorHasSelection"
  },
  
  // NOTE: サイドバーの表示切り替え
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "ctrl+e"
  },
  {
    "command": "workbench.files.action.focusFilesExplorer",
    "key": "ctrl+e",
    "when": "editorTextFocus"
  },
  
  // NOTE: 新規ファイル作成。上のサイドバー表示切り替えと組み合わせて使うことを想定して追加したが、あまり使えていない
  {
    "command": "explorer.newFile",
    "key": "a",
    "when": "filesExplorerFocus && !inputFocus"
  },

  // NOTE: ファイル名の変更。上のサイドバー表示切り替えと組み合わせて使うことを想定して追加したが、あまり使えていない
  {
    "command": "renameFile",
    "key": "r",
    "when": "filesExplorerFocus && !inputFocus"
  },

  // NOTE: ファイルのコピー。上のサイドバー表示切り替えと組み合わせて使うことを想定して追加したが、あまり使えていない
  {
    "command": "filesExplorer.copy",
    "key": "c",
    "when": "filesExplorerFocus && !inputFocus"
  },

  // NOTE: ファイルの貼り付け。上のサイドバー表示切り替えと組み合わせて使うことを想定して追加したが、あまり使えていない
  {
    "command": "filesExplorer.paste",
    "key": "p",
    "when": "filesExplorerFocus && !inputFocus"
  },
  
  // NOTE: ファイルの削除。上のサイドバー表示切り替えと組み合わせて使うことを想定して追加したが、あまり使えていない
  {
    "command": "deleteFile",
    "key": "d",
    "when": "filesExplorerFocus && !inputFocus"
  },
  
  // NOTE: 同じテキストをハイライトしてくれる. 全然使ってない
  {
    "command": "editor.action.addSelectionToNextFindMatch",
    "key": "ctrl+n",
    "when": "editorFocus"
  },

